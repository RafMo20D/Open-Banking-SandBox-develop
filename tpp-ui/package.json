{
  "name": "tpp-ui",
  "version": "4.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4205 --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test-ci": "ng test --browsers ChromeHeadless --watch=false",
    "test-coverage": "ng test --code-coverage",
    "lint": "npx eslint src/app/. ",
    "lint:fix": "npx eslint src/app/. --fix",
    "e2e": "ng e2e",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check ."
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
