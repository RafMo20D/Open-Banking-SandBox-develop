{
  "name": "oba-ui",
  "version": "4.3.0",
  "scripts": {
    "start": "ng serve --port=4400 --proxy-config=proxy.conf.json",
    "load-api": "bash load-api.sh",
    "swagger-gen": "npm run load-api && ng-swagger-gen -i ./src/app/api/swagger/oba.json -o ./src/app/api",
    "remove-and-swagger-gen": "rm -rf ./src/app/api && npm run swagger-gen",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test-with-swagger": "rm -rf ./src/app/api && npm run swagger-gen && ng test",
    "test": "ng test",
    "test-ci": "ng test --browsers ChromeCi --watch=false --code-coverage",
    "lint": "npx eslint src/app/. ",
    "lint:fix": "npx eslint src/app/. --fix",
    "e2e": "ng e2e",
    "prettier": "find . -name '*.js' -or -name '*.ts' -or -name '*.html' -or -name '*.css' -or -name '*.scss'| grep -v build | grep -v \"assets/lib\" | grep -v node_modules | grep -v dist | xargs prettier --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/cdk": "^15.2.8",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/material": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@auth0/angular-jwt": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^14.2.0",
    "@popperjs/core": "^2.11.7",
    "bootstrap": "^5.3.0",
    "browser-detect": "^0.2.28",
    "css-vars-ponyfill": "^2.4.3",
    "date-fns": "^2.17.0",
    "http-status-codes": "^2.1.4",
    "material-icons": "^1.13.5",
    "ng-http-loader": "13.0.0",
    "ng-multiselect-dropdown": "^0.3.7",
    "ngx-clipboard": "^16.0.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.8",
    "@angular/language-service": "^15.2.8",
    "@angular/localize": "^15.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "codelyzer": "^6.0.1",
    "eslint": "^8.13.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-autofix": "^1.1.0",
    "husky": "^4.3.8",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jquery": "^3.5.1",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-swagger-gen": "^2.3.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "puppeteer": "^19.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": ">=4.6.2 <4.9"
  },
  "resolutions": {
    "mixin-deep": "latest"
  }
}
